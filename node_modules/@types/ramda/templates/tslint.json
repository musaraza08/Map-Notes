{
  "extends": "../tslint.json",
  "rules": {
    "no-default-export": false,
    "filename-convention": [
      true,
      {
        "namingStyle": "camelCase",
        "allowPatterns": ["^(__|T|F)(\\.r)?$"],
        "allowPrefixes": ["$"],
        "allowSuffixes": [".c", ".r"]
      }
    ],
    "array-type": [true, "array-simple"],
    "naming-convention": false
  }
}
